		   <groupId>org.hibernate</groupId>
		   <artifactId>hibernate-entitymanager</artifactId>
		   <version>4.2.6.Final</version>
		</dependency>
		<!-- Additional Analyzers: -->
		<dependency>
		   <groupId>org.hibernate</groupId>
		   <artifactId>hibernate-search-analyzers</artifactId>
		   <version>4.4.2.Final</version>
		</dependency>
