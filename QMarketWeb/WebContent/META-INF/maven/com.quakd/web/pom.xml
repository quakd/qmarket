		<dependency>
		   <groupId>org.hibernate</groupId>
		   <artifactId>hibernate-search-infinispan</artifactId>
		   <version>4.4.2.Final</version>
		</dependency>

	    

		<!-- MySQL database driver -->
		<dependency>
			<groupId>mysql</groupId>
